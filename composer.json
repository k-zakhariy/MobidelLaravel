{
    "name": "k-zakhariy/mobidel-laravel",
    "description": "A Laravel package for Service mobidel",
    "keywords": ["laravel", "mobidel"],
    "license": "MIT",
    "authors": [
        {
            "name": "Zakharii Kateryniuk",
            "email": "kzm.pianist@gmail.com",
            "homepage": "https://t.me/zpianist"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "illuminate/support": "5.6.*",
        "guzzlehttp/guzzle": "^6.3",
        "barryvdh/laravel-cors": "^0.11.2"
    },
    "require-dev": {
        "orchestra/testbench": "3.6.*",
        "phpunit/phpunit": "~7.0"
    },
    "autoload": {
        "psr-4": {
            "Zakhariy\\MobidelLaravel\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MobidelTests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Zakhariy\\MobidelLaravel\\MobidelServiceProvider"
            ]
        }
    },

    "minimum-stability": "dev"
}
